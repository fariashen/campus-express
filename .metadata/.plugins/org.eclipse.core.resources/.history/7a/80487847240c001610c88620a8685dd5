package easyCourierHttpPost;

public class UrlFactory {

	/**
	 * 根据请求的不同功能返回不同的Url
	 * 
	 * @param type
	 * @return
	 */
	public static String getUrl(String type) {

		String url = "";

		switch (type) {
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;
		case "Login":

			url = "";
			break;

		}
		return url;
	}
}
